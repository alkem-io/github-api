{
  "name": "@alkemio/github-api",
  "version": "0.1.0",
  "author": "Alkemio Foundation",
  "private": false,
  "license": "EUPL-1.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "data",
    "cli",
    "alkemio"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.prod.json",
    "prebuild": "npm run clean",
    "clean": "rimraf ./dist",
    "format": "prettier --write src/**/*.ts",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "lint:fix": "tsc --noEmit && eslint src/**/*.ts{,x} --fix",
    "nodemon": "nodemon",
    "prepublishOnly": "npm run build",
    "codegen": "graphql-codegen --config codegen.yml",
    "repository-issues": "ts-node-dev src/scripts/repository-issues.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alkem-io/github-api.git"
  },
  "bugs": {
    "url": "https://github.com/alkem-io/github-api/issues"
  },
  "homepage": "https://github.com/alkem-io/github-api#readme",
  "devDependencies": {
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "@graphql-codegen/add": "3.2.3",
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/typescript": "2.8.7",
    "@graphql-codegen/typescript-resolvers": "2.7.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.2.1",
    "ts-node-dev": "^2.0.0"
  },
  "dependencies": {
    "@graphql-codegen/typescript-graphql-request": "^4.5.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "graphql": "^16.6.0",
    "typescript": "^4.7.4",
    "winston": "^3.8.1",
    "dotenv": "^16.0.1",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2"
  },
  "files": [
    "dist/**/*"
  ]
}
